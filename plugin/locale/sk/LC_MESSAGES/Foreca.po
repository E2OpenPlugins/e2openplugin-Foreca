#: ../ui.py:844
msgid ""
"\n"
"<   >       =   Prognosis next/previous day\n"
"0 - 9       =   Prognosis (x) days from now\n"
"\n"
"VOL+/-  =   Fast scroll 100 (City choice)\n"
"Bouquet+/- =   Fast scroll 500 (City choice)\n"
"\n"
"Info        =   This information\n"
"Menu     =   Satellite photos and maps\n"
"\n"
"Red        =   Temperature chart for the upcoming 5 days\n"
"Green    =   Go to Favorite 1\n"
"Yellow    =   Go to Favorite 2\n"
"Blue        =   Go to Home\n"
"\n"
"Wind direction = Arrow to right: Wind from the West"
msgstr ""
"\n"
"<   > = Prognóza na ďalší/predchádzajúcí deň\n"
"0 - 9 = Prognóza na (x) dní\n"
"\n"
"VOL+/- = Rýchly posun 100 (výber mesta)\n"
"Bouquet+/- = Rýchly posun 500 (výber mesta)\n"
"\n"
"Info = Táto informacia\n"
"Menu = Satelitní fotografie a mapy\n"
"\n"
"Červená = Teplota na dalších 5 dní\n"
"Zelená = Prepnúť na Obľúbené 1\n"
"Žlutá = Prepnúť na Obľúbené 2\n"
"Modrá = Prepnúť na Domov\n"
"\n"
"Smer vietoru = šipka doprava: vietor na západ"

